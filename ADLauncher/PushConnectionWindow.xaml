<Window x:Class="ADLauncher.PushConnectionWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ERP登陆" Height="187" Width="393" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <Grid>
        <Label Content="用户名" HorizontalAlignment="Left" Margin="165,53,0,0" VerticalAlignment="Top"/>
        <Label Content="密码" HorizontalAlignment="Left" Margin="165,78,0,0" VerticalAlignment="Top"/>
        <PasswordBox x:Name="passwordbox" HorizontalAlignment="Left" Margin="216,81,0,0" VerticalAlignment="Top" Width="146" Height="22" TabIndex="2"/>
        <TextBox x:Name="tbUserName" HorizontalAlignment="Left" Height="23" Margin="216,53,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="146" TabIndex="1"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="217,110,0,0" VerticalAlignment="Top" Width="70" Click="Button_Click" TabIndex="3"/>
        <Button Content="取消" HorizontalAlignment="Left" Margin="292,110,0,0" VerticalAlignment="Top" Width="70" Click="Button_Click_1" TabIndex="4"/>
        <Image HorizontalAlignment="Left" Height="45" Margin="10,58,0,0" VerticalAlignment="Top" Width="140" Source="Logo.png"/>

    </Grid>
</Window>
